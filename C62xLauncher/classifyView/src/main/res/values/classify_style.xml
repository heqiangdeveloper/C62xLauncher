<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DefaultStyle">
        <item name="MainSpanCount">7</item>
        <item name="SubRatio">60%</item>
        <item name="SubSpanCount">3</item>
        <item name="ShadowColor">#d5292929</item>
        <item name="AnimationDuration">@android:integer/config_mediumAnimTime</item>
        <item name="EdgeWidth">0dp</item>
    </style>
    <style name="InsertAbleGridViewDefaultStyle">
        <item name="RowCount">2</item>
        <item name="ColumnCount">2</item>
        <item name="RowGap">10dp</item>
        <item name="ColumnGap">10dp</item>
        <item name="OutlinePadding">8dp</item>
        <item name="OutlineWidth">1dp</item>
        <item name="OutlineColor">#465364</item>
        <item name="InnerPadding">15dp</item>
    </style>
    <style name="mydialog" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowFrame">@null</item>
    </style>
    <style name="ScrollbarRecyclerView" parent="android:Widget">
        <item name="android:scrollbars">vertical</item>
    </style>
</resources>